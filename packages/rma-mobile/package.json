{
  "name": "rma-mobile",
  "version": "0.0.1",
  "author": "CEPL",
  "homepage": "https://www.castlecraft.in",
  "scripts": {
    "ng": "ng",
    "start": "ng run app:serve --host=localhost --port=8100",
