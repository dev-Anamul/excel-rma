    "@angular/router": "^11.0.9",
    "@ionic-native/browser-tab": "^5.30.0",
    "@ionic-native/core": "^5.30.0",
    "@ionic-native/http": "^5.30.0",
    "@ionic-native/in-app-browser": "^5.30.0",
    "@ionic-native/splash-screen": "^5.30.0",
    "@ionic-native/status-bar": "^5.30.0",
